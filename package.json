{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "hexo clean",
    "build:github": "hexo generate -c _config_github.yml",
    "build:gitee": "hexo generate -c _config_gitee.yml",
    "deploy:github": "hexo deploy -c _config_github.yml",
    "deploy:gitee": "hexo deploy -c _config_gitee.yml",
    "build:both": "npm run clean & npm run build:github & npm run build:gitee",
    "deploy:both": "npm run build:github+deploy & npm run build:gitee+deploy",
    "build:github+deploy": "npm run clean & npm run build:github && npm run deploy:github",
    "build:gitee+deploy": "npm run clean & npm run build:gitee && npm run deploy:gitee",
    "server": "hexo server --config ./_config_github.yml"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "dependencies": {
    "hexo": "^7.3.0",
    "hexo-abbrlink": "^2.2.1",
    "hexo-browsersync": "^0.3.0",
    "hexo-deployer-git": "^4.0.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^6.3.0",
    "hexo-renderer-pug": "^3.0.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.0.0"
  }
}